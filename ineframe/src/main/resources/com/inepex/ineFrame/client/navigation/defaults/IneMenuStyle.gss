@def MENU_FONT_COLOR eval('com.inepex.ineFrame.client.util.DesignConstants.getMenuFontColor()');

.ineMenu {
  width: B5;
  height: 100%;
  background-color: NAVIGATION_DRAWER_BACKGROUND_COLOR;
  font-size: B0D333;
  color: MENU_FONT_COLOR;
}

.ineMenuScrollPanel {
  width: B5;
  height: 100%;
  background-color: NAVIGATION_DRAWER_BACKGROUND_COLOR;
}

.userProfile {
  height: B1D5;
  width: 100%;
  background-color: USER_PROFILE_BACKGROUND_COLOR;
  cursor: pointer;
}

.userProfileImg {
  padding: B0D166;
}

.userProfileEmail {
  font-size: B0D25;
  max-width: B4;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.userProfileName {
  height: 50%;
  font-weight: bold;
  max-width: B4;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.menuSeparatorLbl {
  padding: 0 B0D125;
  text-transform: uppercase;
}

.menuSeparatorPanel {
  width: 100%;
  height: B0D666;
  line-height: B0D666;
  font-size: B0D27;
  background-color: MENU_SEPARATOR_COLOR;
  vertical-align: middle;
}

.menuElementRow {
  width: 100%;
  height: BASE;
  cursor: pointer;
  font-size: B0D291;
}

.menuElementRow:hover {
  background-color: INE_COLOR1;
}

.menuElementIcon {
  text-align: center;
  width: BASE;
  line-height: 0;
}

.menuElementTable {
  width: 100%;
}

.inAliasMode {
  color: INE_COLOR2;
}

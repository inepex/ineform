@def B0D125 eval('com.inepex.ineFrame.client.util.DesignConstants.b0d125WithUnit()');
@def B0D666 eval('com.inepex.ineFrame.client.util.DesignConstants.b0d666WithUnit()');
@def B1D5 eval('com.inepex.ineFrame.client.util.DesignConstants.b1d5WithUnit()');
@def B5 eval('com.inepex.ineFrame.client.util.DesignConstants.b5WithUnit()');

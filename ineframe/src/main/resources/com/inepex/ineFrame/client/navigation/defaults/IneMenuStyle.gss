@def NAVIGATION_DRAWER_BACKGROUND_COLOR eval('com.inepex.ineFrame.client.util.DesignConstants.getNavigationDrawerBackgroundColor()');
@def USER_PROFILE_BACKGROUND_COLOR eval('com.inepex.ineFrame.client.util.DesignConstants.getUserProfileBackgroundColor()');
@def INE_COLOR1 eval('com.inepex.ineFrame.client.util.DesignConstants.getIneColor1()');
@def INE_COLOR2 eval('com.inepex.ineFrame.client.util.DesignConstants.getIneColor2()');

<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="IneFormShowCaseWithEjbs">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>

		<exclude-unlisted-classes>false</exclude-unlisted-classes>

		<properties>
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver" />
			<property name="javax.persistence.jdbc.user" value="IneFormTest" />
			<property name="javax.persistence.jdbc.password" value="IneFormTest" />
			<property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost/IneFormTest" />
		
			<property name="eclipselink.target-database" value="PostgreSQL"/>
			<property name="eclipselink.exclude-eclipselink-orm" value="false"/>
			<property name="eclipselink.logging.level" value="FINE" />
		</properties>
	</persistence-unit>
</persistence>
